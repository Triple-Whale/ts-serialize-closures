  "devDependencies": {
    "@types/node": "^12.0.2",
    "benchmark-octane": "^1.0.0",
    "fs-extra": "^3.0.0",
    "glob": "^7.1.1",
    "things-js": "^2.0.0",
    "ts-node": "^3.0.2",
